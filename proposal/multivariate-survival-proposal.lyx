\begin_layout Plain Layout

survivalgen <- function(beta = c(log(2)), # Covariate coefficients
\end_layout

\begin_layout Plain Layout

                        frailty = c("gamma", "log.normal"), # Frailty distributio
n
\end_layout

\begin_layout Plain Layout

                        censor.mu = 130, # Gaussian distribution for censoring
\end_layout

\begin_layout Plain Layout

                        censor.sigma = 15,
\end_layout

\begin_layout Plain Layout

                        theta = 2, # Frailty distribution parameter
\end_layout

\begin_layout Plain Layout

                        N = 300, K = 2, # Number of families and family
 size
\end_layout

\begin_layout Plain Layout

                        tau = 4.6,
\end_layout

\begin_layout Plain Layout

                        C = 0.01,
\end_layout

\begin_layout Plain Layout

                        covariates = c("normal", "uniform")
\end_layout

\begin_layout Plain Layout

  )
\end_layout

\begin_layout Plain Layout

{
\end_layout

\begin_layout Plain Layout

  # Generate the covariates
\end_layout

\begin_layout Plain Layout

  p <- length(beta)
\end_layout

\begin_layout Plain Layout

  Z <- matrix(0, nrow=N*K, ncol=p)
\end_layout

\begin_layout Plain Layout

  for (j in 1:p) {
\end_layout

\begin_layout Plain Layout

    if (covariates=="normal") {
\end_layout

\begin_layout Plain Layout

      Z[, j] <- rnorm(N*K)
\end_layout

\begin_layout Plain Layout

    } else if (covariates=="uniform") {
\end_layout

\begin_layout Plain Layout

      Z[, j] <- runif(N*K)
\end_layout

\begin_layout Plain Layout

    }
\end_layout

\begin_layout Plain Layout

  }
\end_layout

\begin_layout Plain Layout

  betaZ <- Z%*%beta
\end_layout

\begin_layout Plain Layout

  
\end_layout

\begin_layout Plain Layout

  # Generate the frailty for each family
\end_layout

\begin_layout Plain Layout

  if (frailty=="gamma") {
\end_layout

\begin_layout Plain Layout

    w <- 1
\end_layout

\begin_layout Plain Layout

    if ( theta != 0 ) { w <- rep(rgamma(N, 1/theta, 1/theta), rep(K, N))
 }
\end_layout

\begin_layout Plain Layout

    rate <- exp(betaZ)*w 
\end_layout

\begin_layout Plain Layout

  } else if (frailty=="log.normal") {
\end_layout

\begin_layout Plain Layout

    w <- 1
\end_layout

\begin_layout Plain Layout

    if ( theta != 0 ) { w <- rep(rnorm(N, 0, sqrt(theta)), rep(K, N)) }
\end_layout

\begin_layout Plain Layout

    rate <- exp(betaZ + w)
\end_layout

\begin_layout Plain Layout

  }
\end_layout

\begin_layout Plain Layout

  
\end_layout

\begin_layout Plain Layout

  # Survival time is similar to that of a late onset disease with default
 params
\end_layout

\begin_layout Plain Layout

  u <- runif(N*K)
\end_layout

\begin_layout Plain Layout

  obs.time <- ((-log(u)/rate)^(1/tau))/C
\end_layout

\begin_layout Plain Layout

  
\end_layout

\begin_layout Plain Layout

  # Non-informative right censoring
\end_layout

\begin_layout Plain Layout

  censor.time <- rnorm(N*K, censor.mu, censor.sigma)
\end_layout

\begin_layout Plain Layout

  obs.status <- sign(obs.time <= censor.time)
\end_layout

\begin_layout Plain Layout

  obs.time <- pmin(obs.time, censor.time)
\end_layout

\begin_layout Plain Layout

  
\end_layout

\begin_layout Plain Layout

  colnames(Z) <- paste("Z", 1:p, sep="")
\end_layout

\begin_layout Plain Layout

  dat <- data.frame(family=rep(1:N, rep(K,N)),
\end_layout

\begin_layout Plain Layout

                    rep=rep(1:K, N), 
\end_layout

\begin_layout Plain Layout

                    time=obs.time, 
\end_layout

\begin_layout Plain Layout

                    status=obs.status,
\end_layout

\begin_layout Plain Layout

                    Z)
\end_layout

\begin_layout Plain Layout

  return(dat)
\end_layout

\begin_layout Plain Layout

}
